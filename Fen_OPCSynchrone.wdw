   -
     code : |1+
      Procedure Fen_OPCSynchrone(sNomMachine,sServeurOPC,sIDItem)
      
      // on instancie un serveur Automation
      gautServeur est un objet Automation CLIENT_OPC_AUTOMATION
      gpautGroupe est un objet Automation dynamique
      gpautItem est un objet Automation dynamique
   -
     code : |1+
      // on effectue la connexion
      Connexion()
     type : 34
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1+
      // on effectue la déconnexion
      Deconnexion()
     type : 2
   -
     type : 11
   -
     type : 12
   -
     type : 17
   -
     type : 13
  procedures :
   -
     name : Connexion
     procedure_id : 1542184187514210023
     type_code : 14
     code : |1+
      // Syntaxe :
      //Connexion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure Connexion()
      
      Sablier(Vrai)
      quand exception dans
      	// on se connecte au serveur
      	gautServeur>>Connect((sServeurOPC),(sNomMachine))
      faire
      	erreur("Impossible de se connecter","Veuillez vérifier les paramètres de connexion")
      	ferme()
      fin
      // on crée le groupe d'accès
      gpautGroupe = gautServeur>>OPCGroups>>Add(NOM_GROUPE)
      si gpautGroupe = null alors
      	// le groupe n'a pas pu être créé
      	Erreur("Impossible de créer le groupe "+NOM_GROUPE)
      	retour
      FIN
      // création d'un accès à un item simple
      gpautItem = gpautGroupe>>OPCItems>>AddItem((sIDItem),1)
      si gpautItem = null alors
      	// l'item n'a pas pu être créé
      	Erreur("Impossible de créer l'item "+sIDItem)
      	retour
      FIN
      
      Sablier(Faux)
      
     type : 458752
   -
     name : Déconnexion
     procedure_id : 1542184187514275559
     type_code : 14
     code : |1+
      // Syntaxe :
      //Déconnexion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      Procédure Déconnexion()
      
      // on termine l'utilisation du groupe
      gautServeur>>OPCGroups>>RemoveAll()
      // on se déconnecte du serveur
      gautServeur>>Disconnect()
      
     type : 458752
