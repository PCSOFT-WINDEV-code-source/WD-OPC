     name : OnAsyncWriteComplete
     internal_properties : CAAAAAgAAADoMOJhjsbEBnK1VjTsmLsLlXLLWX8aSwA+N2fHY0+LwZgLqs6Og0BNYDgiCyBhrwSn17B25zv0+rPbgNwPSX6Jy4erZACWbyDq6odBI7XaTTN+3uleOC1IBAtjYlNcKCETegmKE2yoKPjNlv3eky7DdhAXSrvAtoEyCaHYyN/XfPoZBO/bJ6c6aXQWh9upIgYhWOSR
     procedure_id : 1542184041484173378
     type_code : 14
     code : |1+
      // Syntaxe :
      //OnAsyncWriteComplete (<IDTransaction>, <NbItems>, <nTabHandleServeur>, <nTabErreur>)
      //
      // Paramètres :
      // 	IDTransaction : identifiant de la transaction
      // 	NbItems : nombre d'items concernés
      // 	nTabHandleServeur : tableau des handles des items concernés
      // 	nTabErreur : tableaux des erreurs de lecture
      // Valeur de retour :
      // 	Aucune
      
      Procédure OnAsyncWriteComplete(IDTransaction,NbItems <utile>,nTabHandleServeur<utile>,nTabErreur)
      
      // si c'est la bonne transaction
      si IDTransaction = ID_TRANSACTION_ecriture ALORS
      	// on traite le cas d'erreur
      	si nTabErreur[1]<>0 alors
      		// l'item n'a pas pu être lu
      		Erreur("Impossible de lire l'item "+gsIDItem+", erreur n°"+nTabErreur[1])
      		retour
      	FIN
      	// accès à la valeur de l'item
      	EcritureOK = "OK"
      FIN
     type : 458752
   -
     name : OnServerShutDown
     procedure_id : 1542184041484238914
     type_code : 14
     code : |1+
      // Syntaxe :
      //OnServerShutDown (<sRaison>)
      //
      // Paramètres :
      // 	sRaison : raison de l'extinction du serveur
      // Valeur de retour :
      // 	Aucune
      
      Procédure OnServerShutDown(sRaison)
      
      // on ouvre une boite pour afficher la raison
      info("OnServerShutDown : "+sRaison)
     type : 458752
   -
     name : Connexion
     procedure_id : 1542184041484304450
     type_code : 14
     code : |1+
      // Syntaxe :
      //Connexion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      Procédure Connexion()
      
      Sablier(Vrai)
      
      // on se connecte au serveur
      quand exception dans
      	gautServeur>>Connect((gsServeurOPC),(gsNomMachine))
      faire
      	erreur("Impossible de se connecter","Veuillez vérifier les paramètres de connexion")
      	ferme()
      fin
      // on crée le groupe d'accès
      gpautGroupe = gautServeur>>OPCGroups>>Add(NOM_GROUPE)
      si gpautGroupe = null alors
      	// le groupe n'a pas pu être créé
      	Erreur("Impossible de créer le groupe "+NOM_GROUPE)
      	retour
      FIN
      // création d'un accès à un item
      gpautItem = gpautGroupe>>OPCItems>>AddItem((gsIDItem),1)
      si gpautItem = null alors
      	// l'item n'a pas pu être créé
      	Erreur("Impossible de créer l'item "+gsIDItem)
      	retour
      FIN
      
      // branchement des événements sur le serveur
      AutomationEvénement(OnServerShutDown,gautServeur,GUID_INTERFACE_EVENEMENT_SERVEUR_AUTOMATION,1)
      
      // branchement des événements sur les groupes
      AutomationEvénement(OnGlobalDataChange,gautServeur>>OPCGroups,GUID_INTERFACE_EVENEMENT_GROUPES_AUTOMATION,1)
      
      // branchement des événements sur le groupe
      AutomationEvénement(OnDataChange,gpautGroupe,GUID_INTERFACE_EVENEMENT_GROUPE_AUTOMATION,1)
      AutomationEvénement(OnAsyncReadComplete,gpautGroupe,GUID_INTERFACE_EVENEMENT_GROUPE_AUTOMATION,2)
      AutomationEvénement(OnAsyncWriteComplete,gpautGroupe,GUID_INTERFACE_EVENEMENT_GROUPE_AUTOMATION,3)
      gpautGroupe>>IsSubscribed = vrai
      gpautGroupe>>IsActive = vrai
      
      Sablier(Faux)
     type : 458752
   -
     name : Déconnexion
     procedure_id : 1542184041484369986
     type_code : 14
     code : |1+
      // Syntaxe :
      //Déconnexion ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      Procédure Déconnexion()
      
      // on termine l'utilisation du groupe
      gautServeur>>OPCGroups>>RemoveAll()
      // on se déconnecte du serveur
      gautServeur>>Disconnect()
      
     type : 458752
