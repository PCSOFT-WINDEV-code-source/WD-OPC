      procedure Fen_OPCAsynchrone(gsNomMachine,gsServeurOPC,gsIDItem)
      
      // on instancie un serveur Automation
      gautServeur est un objet Automation CLIENT_OPC_AUTOMATION
      gpautGroupe est un objet Automation dynamique
      gpautItem est un objet Automation dynamique
      
      
      constant
      	// identifiant de transaction en lecture
      	ID_TRANSACTION_LECTURE = 1
      	// identifiant de transaction en écriture
      	ID_TRANSACTION_ECRITURE = 2
      FIN
   -
