     name : OnDataChange
     internal_properties : CAAAAAgAAAAVZy9AzVGoOHc15W2vwhe7ZQpnctnqZH0moGLLJBWOmh2CbWAzMyoD6F5Uj8F0+npdRh0HOzHNz/S84LzNVD3fufd6z8INDDBHRLLaACinMFAyUH0xWeZQFI68MffB/LJNiuJVbwu0kH4wv7ZgzLD2cnpYIYnxRKtS9tRZGsSA5kSCqnvmYzYzsNbv3R8Km7EqWtVdEpLaWtBWd3RYd5Bp64mNK4IOcNOLYDQH+rSkh1XqBg==
     procedure_id : 1542184041484042306
     type_code : 14
     code : |1+
      // Syntaxe :
      //OnDataChange (<IDTransaction>, <NbItems>, <nTabHandleServeur>, <vTabValeur>, <nTabQualite>, <rTabTimeStamp>)
      //
      // Paramètres :
      // 	IDTransaction : identifiant de la transaction
      // 	NbItems : nombre d'items concernés
      // 	nTabHandleServeur : tableau des handles des items concernés
      // 	vTabValeur : tableaux des valeurs des items concernés
      // 	nTabQualite : tableaux des qualités de transmission
      // 	rTabTimeStamp : tableaux des timestamps
      // Valeur de retour :
      // 	Aucune
      
      Procédure OnDataChange(IDTransaction,NbItems<utile>,nTabHandleServeur,vTabValeur,nTabQualite,rTabTimeStamp)
      
      // numéro de la transaction
      Transaction_Change = IDTransaction
      // identifiant de l'item
      Item_Change = nTabHandleServeur[1]
      // accès à la valeur de l'item
      ValeurLecture_Change = vTabValeur[1]
      // accès à la qualité de la transmission
      QualitéLecture_change = sGetQualite(nTabQualite[1])
      // accès au "timestamp" de la lecture
      dTimeStamp est une dateheure = rTabTimeStamp[1]
      DateHeureLecture_change = dTimeStamp
      
     type : 458752
   -
     name : OnAsyncReadComplete
     internal_properties : CAAAAAgAAADr4YtsEeH9LhqWghXdCpIruQvbuUmiAJzDDGaOdK+Rl1JsV/qflQc75wkohjfJTiODdezsEbviC8AhkknUu96ohVah4tqailt+5O7G3z0+yw+z+v+usjYKaK5wqpfEzi+SouBy/d7FuQrOMevunzJr98xshmu0dejeiHhIEilQDWe+Yvo6ootMOBpBUDstvbbbhoVWVPEn05jC31VeoDT+FBlpNkQrL0LDaOKeYOsoXuZy
     procedure_id : 1542184041484107842
     type_code : 14
     code : |1+
      // Syntaxe :
      //OnAsyncReadComplete (<IDTransaction>, <NbItems>, <nTabHandleServeur>, <vTabValeur>, <nTabQualite>, <rTabTimeStamp>, <nTabErreur>)
      //
      // Paramètres :
      //	IDTransaction : identifiant de la transaction
      //	NbItems : nombre d'items concernés
      //	nTabHandleServeur : tableau des handles des items concernés
      //	vTabValeur : tableaux des valeurs des items concernés
      //	nTabQualite : tableaux des qualités de transmission
      //	rTabTimeStamp : tableaux des timestamps
      //	nTabErreur : tableaux des erreurs de lecture
      // Valeur de retour :
      // 	Aucune
      
      Procédure OnAsyncReadComplete(IDTransaction,NbItems<utile>,nTabHandleServeur<utile>,vTabValeur,nTabQualite,rTabTimeStamp,nTabErreur)
      
      // si c'est la bonne transaction
      si IDTransaction = ID_TRANSACTION_LECTURE ALORS
      	// on traite le cas d'erreur
      	si nTabErreur[1]<>0 alors
      		// l'item n'a pas pu être lu
      		Erreur("Impossible de lire l'item "+gsIDItem+", erreur n°"+nTabErreur[1])
      		retour
      	FIN
      	// accès à la valeur de l'item
      	ValeurLecture = vTabValeur[1]
      	// accès à la qualité de la transmission
      	QualitéLecture = sGetQualite(nTabQualite[1])
      	// accès au "timestamp" de la lecture
      	dTimeStamp est une dateheure = rTabTimeStamp[1]
      	DateHeureLecture = dTimeStamp
      FIN
     type : 458752
